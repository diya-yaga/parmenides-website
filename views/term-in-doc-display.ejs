<%- include('partials/header') -%>
<body>
    <h1>Parmenides</h1>
    <h3>Term: <%= term %></h3>
    <h3>Document title: 
        <%= docTitle[0].split('","')[0].substring(10) %>
    </h3>
    <h3><a href= <%= url %> >Link to document</a></h3>
    <br>
    <p style="outline: 1px;">
        <% for (var i = 0; i < content.length; i++) { %>
            <%= content[i].substring(12, content[i].length-2) %> 
        <% } %>
    </p>
</body>
</html>